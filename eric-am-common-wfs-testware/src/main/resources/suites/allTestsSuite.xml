<suite name="WFS TestNg Suite" verbose="10" configfailurepolicy="continue" preserve-order="true" thread-count="10">
    <listeners>
        <listener class-name="com.ericsson.amcommonwfs.listeners.WfsTestNGListener"/>
    </listeners>

    <test name="Acceptance tests" parallel="methods">
        <classes>
            <class name="com.ericsson.amcommonwfs.tests.CommonWfsAcceptanceTest"/>
        </classes>
    </test>
</suite>
